
public class Main {
    public static void main(String[] args) {
       Erkarutyun  erkarutyun1=new  Erkarutyun();
       erkarutyun1.Shrjanak=5;
        erkarutyun1.Ankyun=4;
        System.out.println(erkarutyun1.Axexi());


    }

}

class Erkarutyun{
    int Shrjanak=5;;
    int Ankyun=4;


    int Axexi(){
        return Shrjanak+Ankyun;
    }


}


public class Main {
    public static void main(String[] args) {
    Makeres makeres1=new  Makeres();
       makeres1.Shrjanak=5;
        makeres1.Mxoc=4;
        System.out.println(makeres1.Axexi());


    }

}

class Makeres{
    int Shrjanak=5;;
    int Mxoc=4;


    int Axexi(){
        return Shrjanak+Mxoc;
    }


}

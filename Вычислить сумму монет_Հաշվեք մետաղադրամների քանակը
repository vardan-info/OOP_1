public class Main {
    public static void main(String[] args) {
    Metaxadram metaxadram1 = new Metaxadram();
    metaxadram1.arjeq=250;
    metaxadram1.qanak=20;
        System.out.println(metaxadram1.Metaxadrami_hashvum());

    }
}

class Metaxadram{
    int arjeq;
    int qanak;
    int Metaxadrami_hashvum(){
        return arjeq*qanak;
    }
}


public class Main {
    public static void main(String[] args) {
        Xoranarad Xoranarad1=new Xoranarad();
        Xoranarad1.paragic=5;
        Xoranarad1.miavorner=4;
        System.out.println(Xoranarad1.Makeres());


    }

}

class Xoranarad{
    int paragic=5;;
    int miavorner=4;


    int Makeres(){
        return paragic+miavorner;
    }


}
